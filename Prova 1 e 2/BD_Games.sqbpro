(
Cod_jogo INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
Nome_jogo     VARCHAR(50), 
Genero_jogo   VARCHAR(50), 
Plataforma   VARCHAR(50), 
Multiplayer  CHAR(1), 
Preco        NUMERIC(5,2), 
Exclusivo    CHAR(1)
)

CREATE TABLE Console 
(
Cod_console INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT, 
Nome_console        VARCHAR(50), 
Valor_console NUMERIC(5,2), 
Valor_plataforma NUMERIC(6,2)
)

CREATE TABLE Genero 
(
Cod_genero INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT, 
Nome_genero       VARCHAR(50)
)

CREATE TABLE Jogadores 
( 
Usuario INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT, 
Nome_jogador  VARCHAR(80), 
Senha       VARCHAR(15), 
Idade       INT, 
Sexo        CHAR(1), 
Cod_console     INT, 
Genero_preferido VARCHAR(50), 
Qtd_controles    INT, 
Status_online    INT,
FOREIGN KEY (Cod_console) REFERENCES Console (Cod_console)
) 
--OBS: No meu não tem email pois a chave primária que criei é usuario (codigo do jogador)


CREATE TABLE Plataforma 
